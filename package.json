{
  "name": "rn-sliding-view",
  "version": "1.1.0",
  "description": "Customizable sliding view for react-native. Supports different positioning and sizes, allows making itself draggable, etc.",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/DnEgorWeb/rn-sliding-view",
    "type": "git"
  },
  "author": "Egor Deriabin <egor19942012@gmail.com>",
  "license": "MIT",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint index.js",
    "test": "jest"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js",
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "collectCoverage": true,
    "collectCoverageFrom": ["<rootDir>/*.js"],
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "dependencies": {
    "react-native-gesture-responder": "^0.1.1",
    "react-native-responsive-screen": "^1.2.2",
    "react-native-status-bar-height": "^2.3.1"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-native": "^0.60.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "flow-bin": "^0.102.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "jsdom": "^15.1.1",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-native": "^0.60.5"
  },
  "homepage": "https://github.com/DnEgorWeb/rn-sliding-view",
  "keywords": [
    "react-native",
    "sliding",
    "menu",
    "animated",
    "react",
    "ios",
    "android",
    "panel",
    "view"
  ]
}
